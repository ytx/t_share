---
# T-SHARE Staging Secrets Template
# Copy this file to staging_secrets.yml and fill in the actual values
# DO NOT commit the actual secrets file to version control

# Database password
vault_staging_db_password: "your_staging_db_password_here"

# JWT secret for authentication
vault_staging_jwt_secret: "your_staging_jwt_secret_here"

# Google OAuth credentials
vault_staging_google_client_id: "your_staging_google_client_id.apps.googleusercontent.com"
vault_staging_google_client_secret: "your_staging_google_client_secret_here"

# Optional: Encrypt this file with ansible-vault
# Run: ansible-vault encrypt staging_secrets.yml